{
  "version": 2,
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_KEY": "@supabase-key"
  },
  "build": {
    "env": {
      "SUPABASE_URL": "@supabase-url",
      "SUPABASE_KEY": "@supabase-key"
    }
  },
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/**/*.py": {
      "runtime": "python3.9",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
